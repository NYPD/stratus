{
  "manifest_version": 2,
  "name": "stratus",
  "version": "v0.0.1",
  "default_locale": "en",
  "description": "Simple chrome extension that allows you to lower the volume output on SoundCloud.com",
  "page_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_title": "stratus",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://soundcloud.com/*"],
      "js": ["myscript.js"],
      "run_at": "document_start"
    }
  ],
}
