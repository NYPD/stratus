{
  "manifest_version": 2,
  "name": "stratus",
  "version": "0.0.1",
  "description": "Simple extension that allows finer volume control on SoundCloud.com",

  "content_scripts": [{
    "matches": ["*://soundcloud.com/*"],
    "css": ["css/content-script.css"],
    "js": ["js/content-script.js"],
    "run_at": "document_end"
  }],
  "icons": {
    "16": "images/temp.png",
    "32": "images/temp.png",
    "48": "images/temp.png",
    "64": "images/temp.png",
    "128": "images/temp.png"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "images/volume-*.png",
    "images/temp.png",
    "js/stratus-inject.js"
  ]
}
