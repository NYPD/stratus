{
  "manifest_version": 2,
  "name": "stratus",
  "version": "0.0.1",
  "description": "Simple chrome extension that allows you to lower the volume output on SoundCloud.com",
  "browser_action": {
    "default_icon": {
      "16": "images/temp.png",
      "24": "images/temp.png",
      "32": "images/temp.png"
    },
    "default_title": "stratus",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://soundcloud.com/*"],
      "js": ["js/content-script.js"],
      "run_at": "document_start"
    }
  ]
}
